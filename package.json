{
  "name": "electron-ollama-chat",
  "version": "0.0.6",
  "description": "electron-chat",
  "productName": "chat",
  "main": "./out/main/index.js",
  "author": "yaolh",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "publish:win": "npm run build && electron-builder --win -p always --config",
    "publish:mac": "electron-vite build && electron-builder --mac -p always --config",
    "publish:linux": "electron-vite build && electron-builder --linux -p always --config"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@yaolx/utils": "^1.0.1",
    "antd": "^5.10.2",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.1.2",
    "echarts-for-react": "^3.0.1",
    "electron-updater": "^5.3.0",
    "faiss-node": "^0.5.1",
    "gm-crypto": "^0.1.12",
    "inversify": "^6.0.2",
    "knex": "^3.0.1",
    "langchain": "^0.1.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.4",
    "reflect-metadata": "^0.1.13",
    "sql-formatter": "^15.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.9.1",
    "@types/react": "18.2.32",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^2.2.0",
    "electron": "^28.2.10",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5",
    "vite": "^3.2.4"
  }
}
